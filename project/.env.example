# ZDrive Environment Variables Template
# Copy this file to .env and update with your actual values

# AO Process IDs for ZDrive NFT Video Streaming Platform
# These are the deployed process IDs on AO mainnet

# Creator NFT Process - Handles video uploads and creator token minting
VITE_CREATOR_NFT_PROCESS=your_creator_nft_process_id

# Basic Access Process - Handles time-limited video viewing rights (7-90 days)
VITE_BASIC_ACCESS_PROCESS=your_basic_access_process_id

# Premium Access Process - Handles permanent video viewing rights
VITE_PREMIUM_ACCESS_PROCESS=your_premium_access_process_id

# Access Control Process - Central validation system for video viewing permissions
VITE_ACCESS_CONTROL_PROCESS=your_access_control_process_id

# Token Process - AO token process for payments
VITE_TOKEN_PROCESS=xU9zFkq3X2ZQ6olwNVvr1vUWIjc3kXTWr7xKQD6dh10

# Platform Configuration
VITE_PLATFORM_WALLET=your_platform_wallet_address
VITE_UPLOAD_FEE_PERCENTAGE=0.0085
VITE_PLATFORM_FEE_PERCENTAGE=0.10
VITE_CREATOR_SHARE_PERCENTAGE=0.90
VITE_ROYALTY_PERCENTAGE=0.10

# AO Network Configuration (mainnet)
VITE_AO_GATEWAY_URL=https://arweave.net
VITE_AO_CU_URL=https://cu.ao.dev
VITE_AO_MU_URL=https://mu.ao.dev
VITE_AO_SU_URL=https://su.ao.dev

# Development Settings
VITE_NODE_ENV=production
VITE_ENABLE_PRODUCTION_FALLBACK=false

# Livepeer Configuration
NEXT_PUBLIC_LIVEPEER_API_KEY=your_livepeer_api_key_here
VITE_LIVEPEER_API_KEY=your_livepeer_api_key_here